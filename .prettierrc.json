{
  "arrowParens": "always",
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
