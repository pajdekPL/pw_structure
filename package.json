{
  "name": "testing-framework",
  "version": "1.0.0",
  "description": "Awesome testing framework based on PW and TS",
  "dependencies": {
    "axios-debug-log": "^1.0.0",
    "playwright": "^1.46.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.45.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "axios": "^1.6.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "swagger-typescript-api": "^13.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "format:check": "npx prettier . --check \"!**.ts\"",
    "lint": "npx eslint . --ext .ts --max-warnings=0",
    "test": "npx playwright test",
    "test:headed": "npm run test -- --headed",
    "test:ui": "npm run test -- --ui",
    "show-report": "npx playwright show-report",
    "test:tag": "npx playwright test --grep \"@TAG1|@TAG2\"",
    "ut": "jest --verbose"
  }
}
